{
  "name": "lautfm-api",
  "version": "2.0.0",
  "description": "JS wrapper for public laut.fm api",
  "main": "cjs/",
  "module": "esm/",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "npx babel --delete-dir-on-start -d esm/ src/",
    "build:cjs": "npx babel --delete-dir-on-start --env-name cjs -d cjs/ src/",
    "test": "jest"
  },
  "keywords": [
    "lautfm"
  ],
  "repository": "github:fvhockney/lautfm-api",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "author": "Vern Hockney <vern@laut.fm>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.8.0",
    "documentation": "^9.3.1",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2"
  },
  "peerDependencies": {
    "axios": "^0.19.0"
  },
  "dependencies": {}
}
